{
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.insertModeKeyBindings":[
    ],

    "vim.normalModeKeyBindings": [
        {
            "before": ["g","i"],
            "commands": ["editor.action.goToDeclaration"]
        },
        { 
            "before": ["g", "I"],
            "commands": ["workbench.action.navigateBack"]
        }
    ],

    "vim.handleKeys": {
        "<g-I>": false
    },
    "vim.useSystemClipboard": true,
    "vim.leader": "<space>",
    "vim.hlsearch": true,
    "vim.smartRelativeLine": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.fontFamily": " 'SF Mono Regular', 'JetBrains Mono','Droid Sans Mono', 'monospace', 'Droid Sans Fallback'",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "terminal.integrated.fontFamily":"MesloLGS NF",
    "liveshare.notebooks.allowGuestExecuteCells": true,
    "C_Cpp.default.cppStandard": "c++17",
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": false,
    "python.venvPath": "./env",
    "C_Cpp.default.cStandard": "c17",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "yaml": false,
        "javascript": true,
        "c": true,
        "python": true
    },
    "jupyter.askForKernelRestart": false,
    "python.experiments.enabled": false,
    "python.suggest.autoImports": false,
    "window.menuBarVisibility": "toggle",
    "editor.lineNumbers": "relative",
    "svelte.enable-ts-plugin": true,
    "workbench.colorTheme": "Dark+ One Monokai",
    "window.zoomLevel": 1,
    "editor.minimap.autohide": true,
    "prisma.showPrismaDataPlatformNotification": false,
    "colorize.languages": [
        "javascript",
        "json",
        "lua"
    ],
    "makefile.configureOnOpen": true
}
